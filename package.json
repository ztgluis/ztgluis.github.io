{
    "name": "ztgluis-github-io",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "preinstall": "npx rimraf node_modules",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "test": "ng test --code-coverage --browsers ChromeHeadless --watch=false",
        "test:dev": "ng test --code-coverage",
        "lint": "ng lint ztgluis-github-io",
        "lint:fix": "ng lint ztgluis-github-io --fix",
        "e2e": "ng e2e",
        "prettier": "pretty-quick --staged",
        "gh-pages": "ng build --prod && npx angular-cli-ghpages --branch=master",
        "analyze": "ng build --prod --namedChunks=true --statsJson=true && npx webpack-bundle-analyzer dist/stats-es2015.json"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run prettier",
            "pre-push": "npm run lint && npm run test && npm run gh-pages"
        }
    },
    "dependencies": {
        "@angular/animations": "10.1.5",
        "@angular/cdk": "10.2.4",
        "@angular/common": "10.1.5",
        "@angular/compiler": "10.1.5",
        "@angular/core": "10.1.5",
        "@angular/flex-layout": "10.0.0-beta.32",
        "@angular/forms": "10.1.5",
        "@angular/material": "10.2.4",
        "@angular/platform-browser": "10.1.5",
        "@angular/platform-browser-dynamic": "10.1.5",
        "@angular/router": "10.1.5",
        "bootstrap": "4.3.1",
        "rxjs": "6.5.3",
        "tslib": "^2.0.0",
        "zone.js": "^0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1001.5",
        "@angular/cli": "10.1.5",
        "@angular/compiler-cli": "10.1.5",
        "@angular/language-service": "10.1.5",
        "@types/jasmine": "3.4.1",
        "@types/jasminewd2": "2.0.7",
        "@types/node": "12.11.1",
        "codelyzer": "5.1.2",
        "husky": "3.0.7",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "prettier": "1.18.2",
        "pretty-quick": "1.11.1",
        "protractor": "~7.0.0",
        "ts-node": "8.4.1",
        "tslint": "~6.1.0",
        "tslint-config-airbnb": "5.11.2",
        "tslint-config-prettier": "1.18.0",
        "typescript": "4.0.3"
    }
}
